{
  "name": "airtable",
  "version": "0.8.1",
  "license": "MIT",
  "homepage": "https://github.com/airtable/airtable.js",
  "repository": "git://github.com/airtable/airtable.js.git",
  "private": false,
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist --declarationDir dist --declaration true && rollup -c",
    "coverage": "jest --env node --coverage",
    "lint": "eslint .",
    "format": "prettier --write '**/*.js'",
    "test": "npm run coverage && npm run test-unit",
    "test-unit": "jest --env node",
    "prepare": "npm run build",
    "prerelease": "npm run build",
    "postinstall": "npm i --dev && npm run build",
    "copy": "cp -R dist examples/next/lib && cp -R dist examples/nuxt/lib",
    "serve:next": "npm run copy && cd examples/next && npm i && npm run build && npm run start",
    "serve:nuxt": "npm run copy && cd examples/nuxt && npm i && npm run build && npm run start"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "lodash": "^4.17.19",
    "qs": "^6.9.4",
    "retry-axios": "^2.2.1"
  },
  "main": "dist/src/index.js",
  "module": "dist/src/index.es.js",
  "browser": "dist/src/index.iife.js",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE.txt",
    "dist"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "collectCoverageFrom": [
      "src/*.{js,jsx,ts,jtx}",
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.js"
    ],
    "resolver": "jest-pnp-resolver",
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.{ts,tsx}",
      "<rootDir>/**/*.{spec,test}.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.158",
    "@types/qs": "^6.9.4",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "body-parser": "^1.19.0",
    "envify": "^4.1.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.19.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "grunt-browserify": "^5.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-watch-typeahead": "^0.6.0",
    "nock": "^13.0.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rollup": "^2.23.0",
    "rollup-plugin-terser": "^6.1.0",
    "semver": "^7.3.2",
    "serve": "^11.3.2",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "airtable",
    "productivity",
    "database",
    "spreadsheet"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  }
}
